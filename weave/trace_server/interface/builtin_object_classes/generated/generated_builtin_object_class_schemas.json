      "required": [
        "evaluation_object_ref",
        "scorer_name",
        "summary_metric_path"
      ],
      "title": "LeaderboardColumn",
      "type": "object"
    },
    "LlmJudgeActionConfig": {
      "properties": {
        "action_type": {
          "const": "llm_judge",
          "default": "llm_judge",
          "title": "Action Type",
          "type": "string"
        },
        "model": {
          "enum": [
            "gpt-4o",
            "gpt-4o-mini"
