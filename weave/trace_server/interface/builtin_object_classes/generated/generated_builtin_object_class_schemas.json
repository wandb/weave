    "ActionSpec": {
      "properties": {
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Description"
        },
        "config": {
          "discriminator": {
            "mapping": {
              "contains_words": "#/$defs/ContainsWordsActionConfig",
              "llm_judge": "#/$defs/LlmJudgeActionConfig"
            },
            "propertyName": "action_type"
