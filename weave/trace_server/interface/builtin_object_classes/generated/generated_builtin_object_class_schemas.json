            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Prompt"
        },
        "default_params": {
          "$ref": "#/$defs/ModelParams",
          "default": {
            "temperature": null,
            "top_p": null,
            "max_tokens": null,
            "presence_penalty": null,
            "frequency_penalty": null,
            "stop": null
          }
        }
      },
      "required": [
        "provider_model"
      ],
      "title": "LLMModel",
      "type": "object"
    },
    "Leaderboard": {
      "properties": {
