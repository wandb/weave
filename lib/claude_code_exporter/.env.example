# Claude Code Weave Exporter Configuration
# Copy this file to .env and fill in your values

# W&B API key for authentication (required)
CC_WANDB_API_KEY=

# Full project path: entity/project (recommended)
CC_WEAVE_PROJECT=my-team/claude-code-traces

# Or specify entity and project separately:
# CC_WEAVE_ENTITY=my-team
# CC_WEAVE_PROJECT_NAME=claude-code-traces

# Enable/disable tracing (default: true)
# CC_ENABLED=true

# Enable debug logging (default: false)
# CC_DEBUG=true
